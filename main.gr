module Hello

from "./expression.gr" include Tree
from "./lexing.gr" include Lexing
from "list" include List
from "result" include Result

 let state: Lexing.LexingState = {
    position: 0,
    input: "1 + 2 * 3"
 }

let mut token = Result.unwrap(Lexing.nextToken(state))

  while(token.kind != EndOfInput) {
    print(token)

    token = Result.unwrap(Lexing.nextToken(state))
  }
