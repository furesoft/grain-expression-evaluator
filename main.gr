module Hello

from "./expression.gr" include Tree
from "./lexing.gr" include Lexing
from "list" include List

let printTokens = tokens => List.forEach(token => {
    print(token)
}, tokens)

print("start lexing");

match(Lexing.lex("1 + 2 * 3")) {
    Ok(tokens) => printTokens(tokens),
    Err(err) => print(err)
}
